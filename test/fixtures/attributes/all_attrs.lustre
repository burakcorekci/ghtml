@params(
  value: String,
  is_disabled: Bool,
  on_change: fn(String) -> msg,
  on_click: fn() -> msg,
)

<form class="form">
  <input
    type="text"
    class="input"
    value={value}
    disabled
    @input={on_change}
  />
  <button type="submit" @click={on_click()}>
    Submit
  </button>
</form>
